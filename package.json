{
  "name": "whichpm",
  "description": "CLI that Locates and reports information about installed Perl modules.",
  "private": false,
  "version": "0.1.6",
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "preferGlobal": true,
  "bin": {
    "whichpm": "bin/whichpm"
  },
  "homepage": "https://github.com/mklement0/whichpm",
  "repository": {
    "type": "git",
    "url": "https://github.com/mklement0/whichpm.git"
  },
  "bugs": {
    "url": "https://github.com/mklement0/whichpm/issues"
  },
  "scripts": {
