      "manOn": true
    }
  },
  "devDependencies": {
    "doctoc": "^0.13.0",
    "json": "^9.0.3",
    "marked-man": "^0.1.5",
    "replace": "^0.3.0",
    "semver": "^4.2.0",
    "urchin": "^0.0.5"
  }
